<%-include partials/header.ejs%>
<%-include partials/main_nav.ejs%>

<!-- <h1><%= title %></h1>
<p>Welcome to <%= title %></p> -->
<main class="container">
    <div class="jumbotron">
      <h1 class="display-6">Welcome to the <%=title%> page</h1>
    </div>
  
    <div class="d-flex">
      <div class="p-2 flex-fill">
        <div class="card" style="width: 16rem;">
          <img src="../assets/images/juan.jpg" class="card-img-top" alt="my photo">
          <div class="card-body">
            <h5 class="card-title">Juan Arevalo</h5>
            <p class="card-text">Life with challenges is a higher motive to keep evolving .</p>
          </div>
        </div>
      </div>
      <div class="p-2 flex-fill">
        <div class="card">
          <div class="card-header">
            Quote
          </div>
          <div class="card-body">
            <blockquote class="blockquote mb-0">
              <p>A well-known quote, contained in a blockquote element.</p>
              <footer class="blockquote-footer">Someone famous in <cite title="Source Title">Source Title</cite></footer>
            </blockquote>
          </div>
        </div>
        <div class="mb-3">
            <label for="exampleFormControlInput1" class="form-label">Email address</label>
            <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="name@example.com">
          </div>
          <div class="mb-3">
            <label for="exampleFormControlTextarea1" class="form-label">Example textarea</label>
            <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
          </div>
      </div>
      <div class="p-2 flex-fill">
        <div class="list-group">
            <a href="/" class="list-group-item list-group-item-action list-group-item-light text-center">Home</a>
            <a href="/about" class="list-group-item list-group-item-action list-group-item-light text-center">About me</a>
            <a href="projects" class="list-group-item list-group-item-action list-group-item-light text-center">Projects</a>
            <a href="/services" class="list-group-item list-group-item-action list-group-item-light text-center">Services</a>
            <a href="/contact" class="list-group-item list-group-item-action list-group-item-light text-center">Contact me</a>
        </div>
    </div>
    </div>
  </main>




<%-include partials/bottom_nav.ejs%>
<%-include partials/footer.ejs%>